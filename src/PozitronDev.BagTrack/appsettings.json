{
  "KeyVault": {
    "AzureKeyVault": "https://pozitron-bagtrack.vault.azure.net/",
    "DisableAzureKeyVault": "true"
  },
  "BagTrack": {
    "ConnectionString": "Data Source=(localdb)\\MSSQLLocalDB;Initial Catalog=PozitronDev.BagTrack;Integrated Security=SSPI;ConnectRetryCount=0;",
    "DbSecretName": "PozBagTrack-DB"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Error",
      "Microsoft.AspNetCore": "Error"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Error",
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "AllowedHosts": "*"
}
