{
  "KeyVault": {
    "AzureKeyVault": "",
    "DisableAzureKeyVault": "false"
  },
  "BagTrack": {
    "ConnectionString": "",
    "ApiKey": ""
  },
  "MQ": {
    "HostName": "",
    "Port": 0,
    "Channel": "",
    "UserId": "",
    "Password": "",
    "QueueManagerName": "",
    "CCSID": "",
    "InputQueue": "",
    "PollingInterval": 1000
  },
  "Jobs": {
    "DashboardUsername": "admin",
    "DashboardPassword": "",
    "CleanBagsJob": "0 3 * * *",
    "CleanFlightsJob": "10 3 * * *",
    "CleanInboxMessagesJob": "20 3 * * *",
    "CleanOlderThanDays": 30
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Debug", "Serilog.Sinks.File" ],
    "MinimumLevel": "Error",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log.txt",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "AllowedHosts": "*"
}
